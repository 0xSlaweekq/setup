    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "editor.unicodeHighlight.ambiguousCharacters": false, // useful for russian symbols
